<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">


    <fragment
        android:id="@+id/homeFragment"
        android:name="app.com.mapinevents.ui.HomeFragment"
        android:label="Home"
        tools:layout="@layout/home_fragment">
        <action
            android:id="@+id/action_homeFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_homeFragment_to_mapInFragment"
            app:destination="@id/mapInFragment" />
    </fragment>
    <fragment
        android:id="@+id/scheduleFragment"
        android:name="app.com.mapinevents.ui.ScheduleFragment"
        android:label="Schedule"
        tools:layout="@layout/schedule_fragment">
        <action
            android:id="@+id/action_scheduleFragment_to_scheduleDetailFragment"
            app:destination="@id/scheduleDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/agendaFragment"
        android:name="app.com.mapinevents.ui.AgendaFragment"
        android:label="Agenda"
        tools:layout="@layout/agenda_fragment" />
    <fragment
        android:id="@+id/scheduleDetailFragment"
        android:name="app.com.mapinevents.ui.ScheduleDetailFragment"
        android:label="schedule_detail_fragment"
        tools:layout="@layout/schedule_detail_fragment" >
        <action
            android:id="@+id/action_scheduleDetailFragment_to_mapInFragment"
            app:destination="@id/mapInFragment" />
        <argument
            android:name="scheduleModel"
            app:argType="app.com.mapinevents.model.Schedule" />
    </fragment>
    <fragment
        android:id="@+id/mapInFragment"
        android:name="app.com.mapinevents.ui.MapInFragment"
        android:label="Map"
        tools:layout="@layout/map_in_fragment" >
        <action
            android:id="@+id/action_mapInFragment_to_mapInSelectionFragment"
            app:destination="@id/mapInSelectionFragment" />
    </fragment>
    <fragment
        android:id="@+id/moreFragment"
        android:name="app.com.mapinevents.ui.MoreFragment"
        android:label="More"
        tools:layout="@layout/more_fragment">
        <action
            android:id="@+id/action_moreFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_moreFragment_to_infoFragment"
            app:destination="@id/infoFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="app.com.mapinevents.ui.LoginFragment"
        android:label="Login "
        tools:layout="@layout/login_fragment">
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="app.com.mapinevents.ui.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/settings_fragment">
        <action
            android:id="@+id/action_settingsFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_aboutDeveloperTeamFragment"
            app:destination="@id/aboutDeveloperTeamFragment" />
    </fragment>
    <fragment
        android:id="@+id/aboutDeveloperTeamFragment"
        android:name="app.com.mapinevents.ui.AboutDeveloperTeamFragment"
        android:label="about_developer_team_fragment"
        tools:layout="@layout/about_developer_team_fragment" />
    <fragment
        android:id="@+id/infoFragment"
        android:name="app.com.mapinevents.ui.InfoFragment"
        android:label="Info"
        tools:layout="@layout/info_fragment" />
    <fragment
        android:id="@+id/mapInSelectionFragment"
        android:name="app.com.mapinevents.ui.MapInSelectionFragment"
        tools:layout="@layout/map_in_selection_fragment">
        <argument
            android:name="Poi"
            app:argType="app.com.mapinevents.model.POI" />
        <action
            android:id="@+id/action_mapInSelectionFragment_to_mapInNavigationFragment"
            app:destination="@id/mapInNavigationFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapInNavigationFragment"
        android:name="app.com.mapinevents.ui.MapInNavigationFragment">
        <argument
            android:name="Poi"
            app:argType="app.com.mapinevents.model.POI" />
    </fragment>
</navigation>